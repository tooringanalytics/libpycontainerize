{
    "name": "python_uwsgi_django",

    "extends": [
        "python_supervisord",
        "git_mixin",
        "db_mixin",
        "build_mixin"
    ],

    "definition": [
        {
            "name": "uwsgi",
            "required": true,
            "type": "UWSGIConfigDict",
            "default": {}
        },

        {
            "name": "django",
            "required": true,
            "type": "DjangoConfigDict",
            "default": {}
        }
    ],

    "templates": [
        {
            "src": "templates/uwsgi.ini"
        },

        {
            "src": "templates/uwsgi_params"
        }
    ],
}
